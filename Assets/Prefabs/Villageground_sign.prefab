    sprite: {fileID: 21300000, guid: 7d8c3dc0173dc4277817fbb7783246ba, type: 3}
  - name: Elder
    sprite: {fileID: 21300000, guid: d799edfbfcf5341f289854851fca95e7, type: 3}
